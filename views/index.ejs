<!DOCTYPE html>
<html>
  <%- include('./partials/head') %>	
  <body>
    <%- include('./partials/header') %>	
    <div class="content">
      <div class="slider-container">
          <ul class="slider">

              <li id="slide1"><img src="images/Banner/1600338252_757123_1600420348_noticia_normal.jpg"/></li>
              <li id="slide2"><img src="images/Banner/60ab1de8313165e8c70a7632c671e592.png"/></li>
              <li id="slide3"><img src="images/Banner/UJEWTJJ4TJEVPC3K4IO7HVZDAU.jpg"/></li>

          </ul>
          <ul class="slider-menu">

              <li ><a id="slide1" href="#slide1"><i class="fas fa-circle"></i></a></li>
              <li ><a id="slide2" href="#slide2"><i class="fas fa-circle"></i></a></li>
              <li ><a id="slide3" href="#slide3"><i class="fas fa-circle"></i></a></li>
          </ul>
      </div>
      <div class="games">
          <ul class="games-list">

              <!-- <li ><img src="images/Imágenes de Juegos PS4/A Way Out.jpg" alt="A Way Out"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/DiRT 4.jpg" alt="DiRT 4"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/Battlefield 4.jpg" alt="Battlefield 4"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/Little nightmares.jpg" alt="Little nightmares"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/Naruto Shippuden Ultimate Ninja Storm 4.jpg" alt="Naruto Shippuden Ultimate Ninja Storm 4"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/Naruto to Boruto Shinobi Striker.jpg" alt="Naruto to Boruto Shinobi Striker"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/PES 2021.jpg" alt="PES 2021"></li> -->
              <!-- <li ><img src="images/Imágenes de Juegos PS4/Resident Evil 7 Biohazard.jpg" alt="Resident Evil 7"></li> -->
              <% for(let product of products){ %>
                <li >
                  <img src="<%= product.url %> " alt="Assassin’s Creed Valhalla">
                  
                  <div class="game-description">
                      <div class="game-descrition-items">
                          <h2 class="game-name"><%= product.name %></h2>
                          <p class="game-price"><%= product.price %></p>
                      </div>
                      <div class="deatil-buttons">
                          <a href="productDetail?id=<%=product.id%>">Detalle</a > 
                          <a href="../product/productCart">Agregar <i class="fas fa-shopping-cart"></i></a > 
                      </div>
                  </div>  
                </li>
              <% } %> 
          </ul>
      </div>
    </div>
    <%- include('./partials/contacto') %>	 
    <%- include('./partials/footer') %>	
  </body>
</html>
